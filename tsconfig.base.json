{
  "compileOnSave": false,
  "compilerOptions": {
    "rootDir": ".",
    "sourceMap": true,
    "declaration": false,
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "importHelpers": true,
    "target": "es2015",
    "module": "esnext",
    "lib": ["es2020", "dom"],
    "skipLibCheck": true,
    "skipDefaultLibCheck": true,
    "strict": true,
    "baseUrl": ".",
    "paths": {
      "@cat-weight/feature/cat": ["libs/feature/cat/ports/src/index.ts"],
      "@cat-weight/feature/cat/adapters": [
        "libs/feature/cat/adapters/src/index.ts"
      ],
      "@cat-weight/feature/cat/components": [
        "libs/feature/cat/components/src/index.ts"
      ],
      "@cat-weight/feature/cat/internal": [
        "libs/feature/cat/internal/src/index.ts"
      ],
      "@cat-weight/feature/cat/shell": ["libs/feature/cat/shell/src/index.ts"],
      "@cat-weight/feature/weight": ["libs/feature/weight/ports/src/index.ts"],
      "@cat-weight/feature/weight/adapters": [
        "libs/feature/weight/adapters/src/index.ts"
      ],
      "@cat-weight/feature/weight/components": [
        "libs/feature/weight/components/src/index.ts"
      ],
      "@cat-weight/feature/weight/internal": [
        "libs/feature/weight/internal/src/index.ts"
      ],
      "@cat-weight/my-plugin": ["libs/my-plugin/src/index.ts"],
      "@cat-weight/ui/core": ["ui/core/src/index.ts"],
      "@cat-weight/util/routing": ["libs/util/routing/ports/src/index.ts"],
      "@cat-weight/util/routing/adapters": [
        "libs/util/routing/adapters/src/index.ts"
      ],
      "@cat-weight/util/routing/testing": [
        "libs/util/routing/testing/src/index.ts"
      ],
      "@cat-weight/util/core": ["util/core/src/index.ts"],
      "@cat-weight/util/data": ["util/data/src/index.ts"],
      "@cat-weight/util/testing": ["util/testing/src/index.ts"]
    }
  },
  "exclude": ["node_modules", "tmp"]
}
